#include<stdio.h>
int q[5],f=-1,r=-1;
int isFull()
{
    if(r==4)
    return 1;
    else 
    {
        return 0;
    }
}
int isEmpty()
{
    if(f==-1 || f>r)
    return 1;
    else
    return 0;
}
void insert(int e)
{
    if(isFull())
    printf("queue is full\t");
    else
    {
        if(f==-1)
        {
        f++;
        }
        r++;
        q[r]=e;
    }
}
int delete()
{
    if(isEmpty())
    printf("queue is empty\t");
    else
    {
        int t=q[f];
        f++;
        return t;
    }
}
void display()
{
    for(int i=f;i<=r;i++)
    {
        printf("%d\t",q[i]);
    }
}
int main()
{
    insert(10);
    insert(20);
    insert(30);
    insert(40);
    insert(50);
    //insert(60);
    //display();
    delete();
    display();
}
