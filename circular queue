#include<stdio.h>
int cq[100],f=-1,r=-1,e,n;
int isFull()
{
if(f==(r+1)%n)
{
return 1;
}
else
{
return 0;
}
}
int isEmpty()
{
if(f==-1 && r==-1)
{
return 1;
}
else
{
return 0;
}
}
void insert(int e)
{
if(isFull())
{
printf("queue full");
return;
}
if(f==-1 && r==-1)
{
f++;
}
r=(r+1)%n;
cq[r]=e;
}
int delete()
{
int t;
if(isEmpty())
{
printf("queue is empty");
return -99999;
}
if(f==r)
{
t=cq[f];
f=r=-1;
}
f=(f+1)%n;
return t;
}
void printQueue()
{
for(int i=f;i<=n+r;i=i+1)
{
if(i<n)
{
printf("%d\t",cq[i]);
}
else
{
printf("%d\t",cq[i-n]);
}
}
}
int main()
{
    int j;
printf("Enter size n:");
scanf("%d",&n);
for(int j=0;j<n;j++)
{
scanf("%d",&e);
insert(e);
}
delete();
delete();
insert(10);
delete();
printQueue();
return 0;
}
